{
  "name": "jbt-timer-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/cache": "11.7.1",
    "@emotion/react": "11.9.0",
    "@emotion/styled": "11.8.1",
    "@fullcalendar/daygrid": "5.11.0",
    "@fullcalendar/interaction": "5.11.0",
    "@fullcalendar/react": "5.11.1",
    "@fullcalendar/timegrid": "5.11.0",
    "@mui/icons-material": "5.8.2",
    "@mui/material": "5.8.2",
    "@mui/styled-engine": "5.8.0",
    "@pathofdev/react-tag-input": "1.0.7",
    "axios": "1.3.3",
    "cropperjs": "1.5.13",
    "env-cmd": "10.1.0",
    "formik": "2.2.9",
    "moment": "2.29.4",
    "react": "18.2.0",
    "react-cropper": "2.3.2",
    "react-dom": "18.2.0",
    "react-dropzone": "14.2.3",
    "react-quill": "1.3.5",
    "react-redux": "8.0.5",
    "react-router": "6.8.1",
    "react-router-dom": "6.8.1",
    "react-scripts": "5.0.1",
    "react-select": "5.7.0",
    "react-table": "7.8.0",
    "redux": "4.1.0",
    "redux-persist": "6.0.0",
    "redux-persist-transform-encrypt": "4.0.0",
    "redux-saga": "1.2.2",
    "yup": "0.32.11"
  },
  "scripts": {
    "start": "react-scripts start",
    "serve:local": "env-cmd -f .env.local react-scripts start",
    "serve:stage": "env-cmd -f .env.stage react-scripts start",
    "serve:prod": "env-cmd -f .env.prod react-scripts start",
    "build": "react-scripts build --dev=false && cp .htaccess build/.htaccess",
    "build:stage": "env-cmd -f .env.stage yarn build",
    "build:prod": "env-cmd -f .env.prod yarn build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint:check": "eslint \"./src/**/*.{js,jsx}\"",
    "lint:fix": "eslint \"./src/**/*.{js,jsx}\" --fix",
    "format:check": "prettier --check './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "format:fix": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "check-all": "concurrently \"yarn lint:check\" \"yarn format:check\"",
    "prepare": "husky install",
    "prod-setup": "bash script/prod-setup.sh",
    "stage-setup": "bash script/stage-setup.sh"
  },
  "engineStrict": true,
  "engines": {
    "node": "16.x.x",
    "yarn": "1.22.x"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "concurrently": "6.1.0",
    "eslint": "8.0.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.0",
    "lint-staged": "13.1.0",
    "prettier": "2.3.2"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --quiet --cache --fix"
    ],
    "*.{json,js,ts,jsx,tsx,html}": [
      "prettier --write"
    ]
  }
}
